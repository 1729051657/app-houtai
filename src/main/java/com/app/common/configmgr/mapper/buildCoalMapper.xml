<?xml version="1.0" encoding="UTF-8" ?> 
<!DOCTYPE mapper 
       PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
       "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="buildCoalMapper">
	<select id="getBuildCoalList" resultType="com.common.configmgr.pojo.BuildCoal" parameterType="com.common.configmgr.pojo.BuildCoal">
		SELECT * FROM `comm_build_coal_config` WHERE build_id = #{build_id} AND emengry_id = #{emengry_id};
	</select>
</mapper> 